{
  "name": "soundstage",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "npm run build:icons && vite --host",
    "build": "npm run build:icons && vite build",
    "preview": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . -c .eslintrc.js --fix --rulesdir eslint-internal-rules/ --ext .ts,.js,.vue,.tsx,.jsx",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "build:icons": "tsc -b src/@iconify && node src/@iconify/build-icons.js"
  },
  "dependencies": {
    "@tabler/icons": "^1.97.0",
    "@vueuse/core": "^10.0.2",
    "@vueuse/math": "^9.5.0",
    "axios": "^1.4.0",
    "axios-mock-adapter": "^1.21.4",
    "bottom-sheet-vue3": "^2.0.5",
    "pinia": "^2.0.32",
    "pinia-plugin-persistedstate": "^3.1.0",
    "sass": "^1.60.0",
    "swiper": "^9.2.4",
    "unplugin-vue-define-options": "^0.12.8",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6",
    "vue3-otp-input": "^0.4.1",
    "vuetify": "^3.1.16"
  },
  "devDependencies": {
    "@antfu/eslint-config-vue": "^0.31.0",
    "@iconify-json/fa": "^1.1.2",
    "@iconify-json/tabler": "^1.1.35",
    "@iconify/tools": "^2.1.0",
    "@iconify/vue": "^4.0.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/jsdom": "^21.1.0",
    "@types/node": "^18.14.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.3.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.34.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-sonarjs": "^0.16.0",
    "eslint-plugin-unicorn": "^45.0.0",
    "eslint-plugin-vue": "^9.9.0",
    "jsdom": "^21.1.0",
    "npm-run-all": "^4.1.5",
    "typescript": "~4.8.4",
    "unplugin-auto-import": "^0.15.2",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.1.4",
    "vite-plugin-minify": "^1.5.2",
    "vite-plugin-pages": "^0.29.0",
    "vite-plugin-vue-layouts": "^0.8.0",
    "vite-plugin-vuetify": "^1.0.2",
    "vitest": "^0.29.1",
    "vue-tsc": "^1.2.0"
  }
}
